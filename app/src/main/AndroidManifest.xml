        <activity android:name="com.jinzht.pro.activity.ReleaseCircleActivity" />
        <activity android:name="com.jinzht.pro.activity.TranspondCircleActivity" />
        <activity android:name="com.jinzht.pro.activity.CircleDetailActivity" />
        <activity android:name="com.jinzht.pro.activity.ImagePagerActivity" />
        <activity android:name="com.jinzht.pro.activity.ActivityAllApplys" />
        <activity android:name="com.jinzht.pro.activity.ActivityAllComments" />
        <activity android:name="com.jinzht.pro.activity.InvestorgDetailActivity" />
        <activity android:name="com.jinzht.pro.activity.PreselectionAllCommentsActivity"></activity>
    </application>

</manifest>
